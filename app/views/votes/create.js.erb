$("canvas").attr("width","400%")
$("canvas").attr("height","400%")
setTimeout(function(){
  $("canvas").css("display","none")
}, 2000);
<% if current_user.votes.count > 0 %>
  $(".status").html("<%= current_user.name %>さんは「<%= current_user.votes.first.team.name %>」チームに投票しました。投票をやり直すことも出来ます。")
<% else %>
  $(".status").html("<font color='red'>自分のチームには投票できません</font>")
<% end %>